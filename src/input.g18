fn main() -> i32s {
    i32s[][] grid = [
        [1, 2, 3],
        [4, 5, 6],
        [7, 8, 9]
    ];

    i32s sum = 0;
    i32s count = 0;

    for i in 0 to 3 {
        for j in 0 to 3 {
            sum = sum + grid[i][j];
            count = count + 1;
            print(grid[i][j]);
        }
        print("\n");
    }

    print("Sum of 3x3 grid:");
    print(sum);
    print("Count of 3x3 grid:");
    print(count);

    return sum;
}